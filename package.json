{
  "name": "@dytsou/resume-converter",
  "version": "2.0.4",
  "type": "module",
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run convert && vite build",
    "convert": "node scripts/convert-latex.mjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "release": "pnpm run convert && pnpm run build && pnpm run publish",
    "publish:npmjs": "pnpm publish --access public --registry https://registry.npmjs.org"
  },
  "dependencies": {
    "@unified-latex/unified-latex-to-hast": "^1.8.3",
    "@unified-latex/unified-latex-util-parse": "^1.8.3",
    "hast-util-to-html": "^9.0.5",
    "mathjax-full": "^3.2.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rehype-stringify": "^10.0.1",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^16.5.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}