{
  "name": "@dytsou/resume-converter",
  "version": "2.3.0",
  "type": "module",
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run convert && vite build",
    "convert": "node scripts/convert-latex.mjs",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "release": "pnpm run convert && pnpm run build && pnpm run publish",
    "publish:npmjs": "pnpm publish --access public --registry https://registry.npmjs.org",
    "prepare": "command -v husky >/dev/null 2>&1 && husky || true"
  },
  "dependencies": {
    "@unified-latex/unified-latex-to-hast": "^1.8.3",
    "@unified-latex/unified-latex-util-parse": "^1.8.3",
    "hast-util-to-html": "^9.0.5",
    "mathjax-full": "^3.2.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rehype-stringify": "^10.0.1",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.1"
  },
  "license": "MIT",
  "files": [
    "dist",
    "scripts",
    "src",
    "index.html",
    "vite.config.ts",
    "tsconfig.json",
    "tsconfig.app.json",
    "tsconfig.node.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "overrides": {
      "mdast-util-to-hast": "13.2.1"
    }
  }
}
